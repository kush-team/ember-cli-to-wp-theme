{
  "name": "ember-cli-to-wp-theme",
  "version": "1.0.2",
  "description": "This project is intended as a basis for combining Ember as a frontend for a WordPress backend. You can use it as the starting point for a WordPress theme, or a completely standalone app that just retrieves data from WordPress.",
  "repository": "https://github.com/kush-team/ember-cli-to-wp-theme",
  "homepage": "https://github.com/kush-team/ember-cli-to-wp-theme",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember try:testall"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Kush Team, Maximiliano Biondo",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-cli-babel": "^5.0.0",
    "ember-cli": "0.2.3",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "0.0.8",
    "ember-cli-htmlbars": "0.7.4",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.10",
    "ember-cli-uglify": "1.0.1",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-export-application-global": "^1.0.2",
    "ember-data": "1.0.0-beta.16.1",
    "ember-try": "0.0.4"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "rsvp": "^3.0.18",
    "broccoli-funnel": "^0.2.3",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-rename-files": "0.0.3",
    "broccoli-string-replace": "0.0.2",
    "ember-cli-babel": "^5.0.0",
    "ember-cli-htmlbars": "0.7.4",
    "object-assign": "^2.0.0"
